{
  "name": "hessa",
  "version": "1.0.0",
  "description": "Проект на базе Express и TypeScript",
  "main": "dist/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon",
    "dev:old": "ts-node src/index.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "migrate": "ts-node src/services/MigrationService.ts",
    "test:setup": "ts-node scripts/setup-test-db.ts",
    "test": "npm run test:setup && jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test:setup && jest --coverage",
    "test:ci": "npm run test:setup && jest --ci --coverage --watchAll=false"
  },
  "keywords": [
    "express",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.11.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
